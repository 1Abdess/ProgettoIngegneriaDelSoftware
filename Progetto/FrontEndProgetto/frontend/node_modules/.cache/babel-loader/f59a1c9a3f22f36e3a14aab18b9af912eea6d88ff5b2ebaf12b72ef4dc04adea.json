{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport axios from 'axios';\nexport default {\n  data() {\n    return {\n      CodiceFiscale: '',\n      Password: '',\n      error: '' // Aggiunto il dato per gestire il messaggio di errore\n    };\n  },\n  methods: {\n    submitForm() {\n      const formData = {\n        codiceFiscale: this.CodiceFiscale.toUpperCase(),\n        password: this.Password\n      };\n      axios.post('/login', formData).then(response => {\n        // Se la registrazione ha avuto successo, reindirizza a profilazione\n        this.$router.push('/profilazione');\n      }).catch(error => {\n        if (error.response && error.response.data && error.response.data.message) {\n          this.error = error.response.data.message;\n        } else {\n          // Se il messaggio di errore non è disponibile nel formato atteso, mostra un messaggio generico\n          this.error = 'Errore di registrazione, riprova.';\n        }\n        console.error(error);\n      });\n    }\n  }\n};","map":{"version":3,"names":["axios","data","CodiceFiscale","Password","error","methods","submitForm","formData","codiceFiscale","toUpperCase","password","post","then","response","$router","push","catch","message","console"],"sources":["C:\\Users\\abdes\\Desktop\\ProgettoIngegneriaDelSoftware\\Progetto\\FrontEndProgetto\\frontend\\src\\views\\Login.vue"],"sourcesContent":["<template>\r\n  <div class=\"min-h-screen bg-slate-200 py-6 flex flex-col justify-center relative overflow-hidden sm:py-12\">\r\n  \r\n    <span class=\"border text-4xl text-yellow-800 px-6 pt-10 pb-8 bg-white w-1/2 max-w-md mx-auto rounded-t-md sm:px-10\">\r\n      LOGIN\r\n    </span>\r\n  \r\n    <div class=\"border relative px-4 pt-7 pb-8 bg-white shadow-xl w-1/2 max-w-md mx-auto sm:px-10 rounded-b-md\">\r\n        <!-- Utilizziamo la direttiva @submit.prevent per intercettare il submit del form -->\r\n        <form @submit.prevent=\"submitForm\">\r\n            <!-- Aggiunta dell'attributo required per i campi obbligatori -->\r\n            <input v-model=\"CodiceFiscale\" type=\"text\" class=\"border w-full h-10 px-3 mb-5 rounded-md\" placeholder=\"Codice Fiscale\" required> \r\n            <input v-model=\"Password\" type=\"password\" class=\"border w-full h-10 px-3 mb-5 rounded-md\" placeholder=\"Password\" required>\r\n            \r\n            <!-- Cambiato il testo del pulsante a \"Registrati\" -->\r\n            <button type=\"submit\" class=\"mt-5 bg-green-500 hover:bg-blue-700 shadow-xl text-white uppercase text-sm font-semibold px-14 py-3 rounded\">\r\n                Registrati\r\n            </button>\r\n        </form>\r\n    </div>\r\n\r\n    <!-- Aggiunto il blocco per mostrare il messaggio di errore -->\r\n    <div v-if=\"error\" class=\"bg-red-500 text-white px-4 py-2 mt-4 text-center\">\r\n        {{ error }}\r\n    </div>\r\n  </div>\r\n</template>\r\n  \r\n<script>\r\nimport axios from 'axios';\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      CodiceFiscale: '',\r\n      Password: '',\r\n      error: '' // Aggiunto il dato per gestire il messaggio di errore\r\n    };\r\n  },\r\n  methods: {\r\n    submitForm() {\r\n      const formData = {\r\n        codiceFiscale: this.CodiceFiscale.toUpperCase(),\r\n        password: this.Password,\r\n      };\r\n\r\n      axios.post('/login', formData)\r\n        .then(response => {\r\n          // Se la registrazione ha avuto successo, reindirizza a profilazione\r\n          this.$router.push('/profilazione');\r\n        })\r\n        .catch(error => {\r\n          if (error.response && error.response.data && error.response.data.message) {\r\n            this.error = error.response.data.message;\r\n          } else {\r\n            // Se il messaggio di errore non è disponibile nel formato atteso, mostra un messaggio generico\r\n            this.error = 'Errore di registrazione, riprova.';\r\n          }\r\n          console.error(error);\r\n        });\r\n    }\r\n  }\r\n};\r\n</script>\r\n\r\n<style scoped>\r\n/* I tuoi stili rimangono invariati */\r\n</style>\r\n"],"mappings":";AA6BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,EAAE;MACZC,KAAK,EAAE,EAAC,CAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,MAAMC,QAAO,GAAI;QACfC,aAAa,EAAE,IAAI,CAACN,aAAa,CAACO,WAAW,CAAC,CAAC;QAC/CC,QAAQ,EAAE,IAAI,CAACP;MACjB,CAAC;MAEDH,KAAK,CAACW,IAAI,CAAC,QAAQ,EAAEJ,QAAQ,EAC1BK,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,eAAe,CAAC;MACpC,CAAC,EACAC,KAAK,CAACZ,KAAI,IAAK;QACd,IAAIA,KAAK,CAACS,QAAO,IAAKT,KAAK,CAACS,QAAQ,CAACZ,IAAG,IAAKG,KAAK,CAACS,QAAQ,CAACZ,IAAI,CAACgB,OAAO,EAAE;UACxE,IAAI,CAACb,KAAI,GAAIA,KAAK,CAACS,QAAQ,CAACZ,IAAI,CAACgB,OAAO;QAC1C,OAAO;UACL;UACA,IAAI,CAACb,KAAI,GAAI,mCAAmC;QAClD;QACAc,OAAO,CAACd,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}