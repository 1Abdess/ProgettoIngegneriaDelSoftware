form @submit.prevent=\"submitForm\">\n            <!-- Aggiunta dell'attributo required per i campi obbligatori -->\n            <input v-model=\"CodiceFiscale\" type=\"text\" class=\"border w-full h-10 px-3 mb-5 rounded-md\" placeholder=\"Codice Fiscale\" required> \n            <input v-model=\"Password\" type=\"password\" class=\"border w-full h-10 px-3 mb-5 rounded-md\" placeholder=\"Password\" required>\n            \n            <!-- Cambiato il testo del pulsante a \"Registrati\" -->\n            <button type=\"submit\" class=\"mt-5 bg-green-500 hover:bg-blue-700 shadow-xl text-white uppercase text-sm font-semibold px-14 py-3 rounded\">\n                Registrati\n            </button>\n        </form>\n    </div>\n\n    <!-- Aggiunto il blocco per mostrare il messaggio di errore -->\n    <div v-if=\"error\" class=\"bg-red-500 text-white px-4 py-2 mt-4 text-center\">\n        {{ error }}\n    </div>\n  </div>\n</template>\n  \n<script>\nimport axios from 'axios';\n\nexport default {\n  data() {\n    return {\n      CodiceFiscale: '',\n      Password: '',\n      Nome: '',\n      Cognome: '',\n      Email: '',\n      Organizzazione: '',\n      Categoria: '',\n      FotoProfilo: '',\n      error: '' // Aggiunto il dato per gestire il messaggio di errore\n    };\n  },\n  methods: {\n    submitForm() {\n      const formData = {\n        CodiceFiscale: this.CodiceFiscale,\n        Password: this.Password,\n        Nome: this.Nome,\n        Cognome: this.Cognome,\n        Email: this.Email,\n        Organizzazione: this.Organizzazione,\n        Categoria: this.Categoria,\n        FotoProfilo: this.FotoProfilo\n      };\n\n      axios.post('/login', formData)\n        .then(response => {\n          // Se la registrazione ha avuto successo, reindirizza a profilazione\n          this.$router.push('/profilazione');\n        })\n        .catch(error => {\n          // Se la registrazione non ha avuto successo, mostra un messaggio di errore\n          this.error = 'Errore di registrazione, riprova.';\n          console.error(error);\n        });\n    }\n  }\n};\n</script>\n\n<style scoped>\n/* I tuoi stili rimangono invariati */\n</style>\n"],"mappings":";AA6BA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,aAAa,EAAE,EAAE;MACjBC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,OAAO,EAAE,EAAE;MACXC,KAAK,EAAE,EAAE;MACTC,cAAc,EAAE,EAAE;MAClBC,SAAS,EAAE,EAAE;MACbC,WAAW,EAAE,EAAE;MACfC,KAAK,EAAE,EAAC,CAAE;IACZ,CAAC;EACH,CAAC;EACDC,OAAO,EAAE;IACPC,UAAUA,CAAA,EAAG;MACX,MAAMC,QAAO,GAAI;QACfX,aAAa,EAAE,IAAI,CAACA,aAAa;QACjCC,QAAQ,EAAE,IAAI,CAACA,QAAQ;QACvBC,IAAI,EAAE,IAAI,CAACA,IAAI;QACfC,OAAO,EAAE,IAAI,CAACA,OAAO;QACrBC,KAAK,EAAE,IAAI,CAACA,KAAK;QACjBC,cAAc,EAAE,IAAI,CAACA,cAAc;QACnCC,SAAS,EAAE,IAAI,CAACA,SAAS;QACzBC,WAAW,EAAE,IAAI,CAACA;MACpB,CAAC;MAEDT,KAAK,CAACc,IAAI,CAAC,QAAQ,EAAED,QAAQ,EAC1BE,IAAI,CAACC,QAAO,IAAK;QAChB;QACA,IAAI,CAACC,OAAO,CAACC,IAAI,CAAC,eAAe,CAAC;MACpC,CAAC,EACAC,KAAK,CAACT,KAAI,IAAK;QACd;QACA,IAAI,CAACA,KAAI,GAAI,mCAAmC;QAChDU,OAAO,CAACV,KAAK,CAACA,KAAK,CAAC;MACtB,CAAC,CAAC;IACN;EACF;AACF,CAAC","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}